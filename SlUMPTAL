using System;
using System.Diagnostics;
using System.Linq;

namespace Insertionsort_Complete
{
    public class Program
    {
         static void Main(string[] args)
         {
        Random rnd = new Random();
        int length = 100;
        int[] arr = new int[length];

        for(int i = 0; i < arr.Length; i++)
        {
            arr[i] = rnd.Next();
        }
        foreach(int e in arr)
        {
            Console.WriteLine(e);
        }

        }
    }
}
